<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textView_dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:text="@string/begenenler"
        android:textStyle="bold" />

    <ListView
        android:id="@+id/dialog_listview_likes"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:dividerHeight="1dp"
        android:layout_marginTop="20dp"
        android:divider="@drawable/list_divider"
        android:layout_below="@id/textView_dialog"
        android:layout_centerHorizontal="true">

    </ListView>

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kapat"
        android:layout_below="@id/dialog_listview_likes"
        android:layout_centerHorizontal="true"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="244dp" />
</RelativeLayout>